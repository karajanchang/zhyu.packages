{
  "name": "zhyu/packages",
  "description": "some tools for laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "repository",
    "repositories",
    "eloquent",
    "database"
  ],
  "authors": [
    {
      "name": "karajanchang",
      "email": "karajanchang@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/auth": "^6.0|^7.0",
    "illuminate/config": "^6.0|^7.0",
    "illuminate/database": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0",
    "maatwebsite/excel": "^3.1",
    "league/csv": "^9.2",
    "laravel/helpers": "^1.1"
  },
  "autoload": {
     "psr-4": {
       "Zhyu\\": "src/"
    }
  },
  "autoload-dev": {
     "psr-4": {
       "Zhyu\\Tests\\Packages\\": "tests/"
    }
  },
  "extra": {
     "branch-alias": {
       "dev-master": "0.2.5-dev"
    },
    "laravel": {
       "providers": [
         "Zhyu\\ZhyuServiceProvider"
       ],
      "aliases": {
        "PdfReport": "Zhyu\\Facades\\PdfReport",
        "ExcelReport": "Zhyu\\Facades\\ExcelReport",
        "CsvReport": "Zhyu\\Facades\\CsvReport::class"
      }
    }
  },
  "prefer-stable": true,
  "require-dev": {
     "phpunit/phpunit": "^8.0"
  }
}
